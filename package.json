{
  "name": "google-domain-filter",
  "version": "1.2.0",
  "description": "Filter and highlight domains in your google searches.",
  "main": "index.js",
  "scripts": {
    "watch": "npm run clean && webpack --env mode=development --env watch",
    "build": "npm run clean && webpack --env mode=production && web-ext build -s build -a dist -o",
    "start": "web-ext run -t firefox-desktop -f firefoxdeveloperedition -t chromium -u google.com --source-dir build",
    "start:firefox": "web-ext run -t firefox-desktop -f firefoxdeveloperedition -u google.com --source-dir build",
    "start:chrome": "web-ext run -t chromium -u google.com --source-dir build",
    "clean": "rimraf build",
    "test": "jest"
  },
  "author": "Kyle Carhart",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/helper-compilation-targets": "^7.14.4",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.14.5",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.9",
    "@types/react-beautiful-dnd": "^13.0.0",
    "@types/react-dom": "^17.0.7",
    "@types/styled-components": "^5.1.10",
    "@types/validator": "^13.1.4",
    "babel-jest": "^27.0.5",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "copy-webpack-plugin": "^9.0.0",
    "css-loader": "^5.2.6",
    "eslint": "^7.28.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^27.0.5",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "ts-jest": "^27.0.3",
    "web-ext": "^6.1.0",
    "webextension-polyfill-ts": "^0.25.0",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.2"
  },
  "dependencies": {
    "@popperjs/core": "^2.9.2",
    "@tippyjs/react": "^4.2.5",
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^17.0.2",
    "react-popper": "^2.2.5",
    "reset-css": "^5.0.1",
    "source-map-loader": "^3.0.0",
    "styled-components": "^5.3.0",
    "ts-loader": "^9.2.3",
    "validator": "^13.6.0"
  }
}
